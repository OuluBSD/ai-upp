#!/bin/bash
# Test script for VfsShell logical operators

echo "Testing VfsShell logical operators..."

# Test basic echo
echo "Test 1: Basic echo command"
timeout 2s /home/sblo/Dev/ai-upp/bin/VfsShell echo "Hello World"

# Test true command
echo "Test 2: True command"
timeout 2s /home/sblo/Dev/ai-upp/bin/VfsShell true

# Test false command  
echo "Test 3: False command"
timeout 2s /home/sblo/Dev/ai-upp/bin/VfsShell false

# Test logical AND where first succeeds
echo "Test 4: Logical AND (true && echo)"
timeout 2s /home/sblo/Dev/ai-upp/bin/VfsShell sh -c "true && echo 'SUCCESS'"

# Test logical AND where first fails
echo "Test 5: Logical AND (false && echo)"
timeout 2s /home/sblo/Dev/ai-upp/bin/VfsShell sh -c "false && echo 'SHOULD_NOT_SEE_THIS'"

# Test logical OR where first fails
echo "Test 6: Logical OR (false || echo)"  
timeout 2s /home/sblo/Dev/ai-upp/bin/VfsShell sh -c "false || echo 'SUCCESS'"

# Test logical OR where first succeeds
echo "Test 7: Logical OR (true || echo)"
timeout 2s /home/sblo/Dev/ai-upp/bin/VfsShell sh -c "true || echo 'SHOULD_NOT_SEE_THIS'"

echo "All tests completed"