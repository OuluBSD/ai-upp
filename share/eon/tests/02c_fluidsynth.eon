machine midi.app:
	
	chain program:
		
		loop event:
			center.customer
			midi.file.reader[][loop == "input"]:
				filepath = "midi/saturday_show.mid"
				close_machine = true
		
		loop input:
			center.customer
			fluidsynth.pipe[loop == "event"]:
				verbose = false
				queue = 10
				debug_sound_output = ""
				debug_sound_seed = 0
			center.audio.sink.hw:
				debug_sound_output = ""
				debug_sound_seed = 0
