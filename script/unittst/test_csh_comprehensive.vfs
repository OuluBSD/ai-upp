# VfsShell test script - Comprehensive test of csh functionality
echo "=== VfsShell csh/tcsh Syntax Tests ==="

echo "Test: Basic command execution"
echo "Hello from VfsShell!"

echo "Test: Testing true and false commands"
true
echo "Exit code after true: $?"  # This won't work yet as we don't have $? implemented
false
echo "Exit code after false: $?"  # This won't work yet as we don't have $? implemented

echo "Test: Logical AND operator"
true && echo "SUCCESS: true && echo worked"
false && echo "SHOULD NOT APPEAR: false && echo"

echo "Test: Logical OR operator"
false || echo "SUCCESS: false || echo worked"
true || echo "SHOULD NOT APPEAR: true || echo"

echo "Test: Complex expressions"
true && false || echo "This should appear due to OR"
false && echo "This should NOT appear" || echo "This SHOULD appear"

echo "Test: Command sequences"
echo "first"; echo "second"; echo "third"

echo "Test: Pipelines (basic)"
echo "hello world" | echo "piping test"

echo "All tests completed successfully!"