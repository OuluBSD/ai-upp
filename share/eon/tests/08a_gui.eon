// GUI Event System Test: form file display using net-based format

machine upp.app:
	net gui.test:
		center.customer
		center.gui.filesrc:
			file = "share/eon/forms/test_form.form"
		upp.gui.sink:
			close_machine = true
			sizeable = true
			env = "/event/register"

		center.customer.0 -> center.gui.filesrc.0
		center.gui.filesrc.0 -> upp.gui.sink.0
		upp.gui.sink.0 -> center.customer.0
